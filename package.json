{
  "name": "inspect-x",
  "version": "1.0.9",
  "description": "An implementation of node's ES6 inspect module.",
  "homepage": "https://github.com/Xotic750/inspect-x",
  "author": {
    "name": "Graham Fairweather",
    "email": "xotic750@gmail.com"
  },
  "copyright": "Copyright (c) 2015",
  "keywords": [
    "ES6",
    "inspect",
    "module",
    "javascript",
    "nodejs",
    "browser"
  ],
  "main": "index.js",
  "engines": {
    "node": "*"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Xotic750/inspect-x.git"
  },
  "bugs": {
    "url": "https://github.com/Xotic750/inspect-x/issues"
  },
  "dependencies": {
    "es-abstract": "^1.5.0",
    "define-properties": "^1.1.2",
    "is-date-object": "^1.0.1",
    "is-typed-array": "^1.0.3",
    "is-map-x": "^1.0.4",
    "is-set-x": "^1.0.4",
    "is-primitive": "^2.0.0",
    "has-own-property-x": "^1.0.4",
    "has-symbol-support-x": "^1.0.5",
    "is-array-buffer-x": "^1.0.6",
    "is-data-view-x": "^1.0.7",
    "is-nil-x": "^1.0.4",
    "validate.io-undefined": "^1.0.3",
    "lodash.isnull": "^3.0.0",
    "is-error-x": "^1.0.7",
    "is-object-like-x": "^1.0.4",
    "get-function-name-x": "^1.0.4",
    "is-promise": "^2.1.0",
    "white-space-x": "^1.0.2"
  },
  "devDependencies": {
    "es5-shim": "^4.4.1",
    "es6-shim": "^0.34.1",
    "json3": "^3.3.2",
    "jscs": "^2.8.0",
    "uglify-js": "^2.6.1",
    "browserify": "^13.0.0",
    "browserify-derequire": "^0.9.4",
    "jasmine-node": "^1.14.5",
    "jsdoc-to-markdown": "^1.3.3",
    "nsp": "^2.2.0"
  },
  "scripts": {
    "test": "jasmine-node --matchall tests/spec/",
    "security": "nsp check",
    "style": "jscs index.js",
    "docs": "jsdoc2md --name-format --example-lang js index.js > README.md",
    "build": "browserify -p browserify-derequire -e index.js -o lib/inspect-x.js -u 'crypto' -s returnExports",
    "build-uglify": "uglifyjs lib/inspect-x.js -o lib/inspect-x.min.js --compress --keep-fnames --mangle --beautify ascii_only=true,beautify=false --source-map lib/inspect-x.map"
  }
}
