{
    "name": "inspect-x",
    "version": "0.0.1",
    "description": "An implementation of node's inspect module.",
    "homepage": "https://github.com/Xotic750/inspect-x",
    "author": {
        "name": "Graham Fairweather",
        "email": "xotic750@gmail.com"
    },
    "copyright": "Copyright (c) 2015",
    "keywords": [
        "inspect",
        "module",
        "javascript",
        "nodejs",
        "browser"
    ],
    "main": "index.js",
    "engines": {
        "node": "*"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Xotic750/inspect-x.git"
    },
    "bugs": {
        "url": "https://github.com/Xotic750/inspect-x/issues"
    },
    "dependencies": {
      "es-abstract": "^1.4.3",
      "is-date-object": "^1.0.1",
      "to-string-tag-x": "^1.0.0",
      "is-array-buffer": "^0.1.0",
      "is-typedarray": "^1.0.0",
      "is-object": "^1.0.1",
      "has-own-property-x": "^1.0.0",
      "object.assign": "4.0.3",
      "lodash.isfunction": "^3.0.6"
    },
    "devDependencies": {
      "es5-shim": "^4.3.1",
      "jscs": "^2.5.1",
      "uglify-js": "^2.6.1",
      "browserify": "12.0.1",
      "jasmine-node": "^1.14.5",
      "jsdoc-to-markdown": "^1.2.1"
    },
    "scripts": {
      "test": "jasmine-node --matchall tests/spec/",
      "style": "jscs index.js",
      "docs": "jsdoc2md --name-format code --example-lang js table index.js > README.md",
      "build": "browserify -e index.js -o lib/inspect-x.js -u 'crypto' -s returnExports",
      "build-uglify": "uglifyjs lib/inspect-x.js -o lib/inspect-x.min.js --compress unused=false,dead_code=false --keep-fnames --mangle --beautify ascii_only=true,beautify=false --source-map lib/inspect-x.map"
    }
}
