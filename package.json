  "description": "An implementation of node's ES6 inspect module.",
  "homepage": "https://github.com/Xotic750/inspect-x",
  "author": {
    "name": "Graham Fairweather",
    "email": "xotic750@gmail.com"
  },
  "copyright": "Copyright (c) 2015-2017",
  "keywords": [
    "ES6",
    "inspect",
    "module",
    "javascript",
    "nodejs",
    "browser"
  ],
  "main": "index.js",
  "engines": {
    "node": "*"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Xotic750/inspect-x.git"
  },
  "bugs": {
    "url": "https://github.com/Xotic750/inspect-x/issues"
  },
  "dependencies": {
    "array-includes": "^3.0.2",
    "define-properties-x": "^1.3.0",
    "foreach": "^2.0.5",
    "get-function-name-x": "^1.2.0",
    "has-own-property-x": "^1.2.0",
    "has-symbol-support-x": "^1.2.0",
    "index-of-x": "^1.2.1",
    "is-array-buffer-x": "^1.2.1",
    "is-async-function-x": "^1.0.1",
    "is-boolean-object": "^1.0.0",
    "is-data-view-x": "^1.2.1",
    "is-date-object": "^1.0.1",
    "is-error-x": "^1.2.0",
    "is-function-x": "^1.2.0",
    "is-generator-function": "^1.0.6",
    "is-map-x": "^1.2.0",
    "is-nan": "^1.2.1",
    "is-negative-zero": "^2.0.0",
    "is-nil-x": "^1.2.0",
    "is-number-object": "^1.0.3",
    "is-object-like-x": "^1.2.0",
    "is-primitive": "^2.0.0",
    "is-promise": "^2.1.0",
    "is-regex": "^1.0.4",
    "is-set-x": "^1.2.0",
    "is-string": "^1.0.4",
    "is-symbol": "^1.0.1",
    "is-typed-array": "^1.0.4",
    "json3": "^3.3.2",
    "lodash._arrayfilter": "^3.0.0",
    "lodash.isnull": "^3.0.0",
    "object-keys": "^1.0.11",
    "object.assign": "^4.0.4",
    "reduce": "^1.0.1",
    "to-string-tag-x": "^1.2.0",
    "validate.io-array": "^1.0.6",
    "validate.io-undefined": "^1.0.3"
  },
  "devDependencies": {
    "@ljharb/eslint-config": "^11.0.0",
    "browserify": "^14.1.0",
    "browserify-derequire": "^0.9.4",
    "es5-shim": "^4.5.9",
    "es6-shim": "^0.35.3",
    "es7-shim": "^6.0.0",
    "eslint": "^3.18.0",
    "eslint-plugin-import": "^2.2.0",
    "jasmine-node": "^1.14.5",
    "jsdoc-to-markdown": "^3.0.0",
    "make-jasmine-spec-runner-html": "^1.1.0",
    "nodemon": "^1.11.0",
    "nsp": "^2.6.3",
    "replace-x": "^1.1.1",
    "uglify-js": "^2.8.20"
  },
  "scripts": {
    "build": "npm run build:setver && npm run security && npm run eslint && npm run browserify && npm run uglify && npm run docs && npm run build:jasmine && npm test",
    "build:jasmine": "make-jasmine-spec-runner-html",
    "build:setver": "PKG_VER=$(node -p -e \"require('./package.json').version\") && replace-x \" @version .*\" \" @version ${PKG_VER}\" index.js",
    "build:watch": "nodemon --watch index.js --watch package.json --exec 'npm run build'",
    "docs": "jsdoc2md --name-format --example-lang js index.js > README.md",
    "eslint": "eslint *.js tests/spec/*.js",
    "security": "nsp check",
    "test": "jasmine-node --matchall tests/spec/",
    "test:watch": "nodemon --watch tests/spec/test.js --exec 'npm test'",
    "browserify": "browserify -p browserify-derequire -e index.js -o lib/inspect-x.js -u 'crypto' -s returnExports",
    "uglify": "uglifyjs lib/inspect-x.js -o lib/inspect-x.min.js --compress --keep-fnames --mangle --beautify ascii_only=true,beautify=false --source-map lib/inspect-x.map"
  }
}
